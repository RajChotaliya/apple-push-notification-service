{
    "name": "rajchotaliya/apple-push-notification-service",
    "description": "A PHP package to send push notifications using Apple's APNs.",
    "type": "library",
    "require": {
        "php": ">=7.4"
    },
    "autoload": {
        "psr-4": {
            "RajChotaliya\\ApplePushNotificationService\\": "src/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Raj Chotaliya",
            "email": "rajkc.webdev@gmail.com"
        }
    ],
   "extra": {
        "laravel": {
            "providers": [
                "RajChotaliya\\ApplePushNotificationService\\ApplePushNotificationServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "version": "1.0.7",
    "repositories": [
        {
            "type": "vcs",
